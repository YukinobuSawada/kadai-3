<h2>Users</h2>
<% users.each do |user| %>
<table class="table">
	<thead>
		<tr>
			<th>image</th>
			<th>name</th>
			<th> </th>
		</tr>
	</thead>
		<tr>
			<td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpeg' ,fallback: "no_image.jpg" ,size:'40x40'%></td>
			<td><%= user.name %></td>
    			</td>
			<td><%= link_to"Show",user_path(user.id) %></td>
		</tr>
<% end %>
</table>